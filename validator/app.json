{
  "expo": {
    "name": "INVENTO Validator",
    "slug": "invento-validator",
    "version": "1.0.0",
    "assetBundlePatterns": [
      "**/*"
    ],
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "plugins": [
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Allow INVENTO Validator to access your camera to scan QR codes."
        }
      ]
    ],
    "platforms": [
      "ios",
      "android",
      "web"
    ]
  }
}
